! clear up
clear lock
clear mod1
clear mod4

! remap
!! capslock to super
keycode 66 = Escape
!! alt to super
keycode 64 =  Super_L
keycode 108 =  Super_R
!! super to Alt 
keycode 133 = Alt_L 
!! idk to ctrl
keycode 135 = Control_R

! modifiers
add mod1 = Super_L Super_R
add mod4 = Alt_L Alt_R Escape
add control = Control_R Control_L
