# execs
alias edit="nvim"
# git
alias gcommit="git commit"
alias gpull="git pull"
alias gpush="git push"
alias gadd="git add"
alias gfetch="git fetch"
alias gclone="git clone" 
#nvim
alias vim="nvim"
alias vi="nvim"
alias v="nvim"
#ranger
alias r="ranger"
#xclip
alias xclip="xclip -selection c"
# yad
alias yad="yad --center"
# enable switch dir
alias p=". p"
# diff 
alias diff="git diff --no-index"
# catkin_make
alias catkin_make="catkin_make --directory "$HOME/catkin_ws" -DCMAKE_EXPORT_COMPILE_COMMANDS=1"

alias gdiff="git diff --color-words=."

